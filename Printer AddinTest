# Printer 1
$printer1Name = "Copier-Production on BOS-FS2"
$printer1IP = "192.168.4.10"
$printer1Port = "IP_192.168.4.10"
$printer1Driver = "TOSHIBA e-STUDIO Universal PCL6"

Add-PrinterPort -Name $printer1Port -PrinterHostAddress $printer1IP
Add-Printer -Name $printer1Name -DriverName $printer1Driver -PortName $printer1Port
